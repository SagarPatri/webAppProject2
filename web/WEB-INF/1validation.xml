<form-validation>
  <global>
     <!-- regular expression for alpha numeric fields without spance-->
    <constant>
      <constant-name>alphaNumericWithoutSpace</constant-name>
      <constant-value>^[a-zA-Z0-9]*$</constant-value>
    </constant>
    <!-- regular expression for alpha numeric fields -->
    <constant>
      <constant-name>alphaNumeric</constant-name>
      <constant-value>^[a-zA-Z0-9]{1}[a-zA-Z0-9\s]*$</constant-value>
    </constant>
    <!-- regular expression for phone number  fields -->
    <constant>
      <constant-name>checkPhoneNo</constant-name>
      <constant-value>^[0-9\(]{1}[0-9\s\-\(\)\/\.]*$</constant-value>
    </constant>
    <!-- regular expression for policy/account number  fields -->
    <constant>
      <constant-name>checkNumber</constant-name>
      <constant-value>^[a-zA-Z0-9]{1}[a-zA-Z0-9\s\-\/\\]*$</constant-value>
    </constant>
    <!-- regular expression for organization names fields -->
    <constant>
      <constant-name>checkOrgName</constant-name>
      <constant-value>^[a-zA-Z]{1}[a-zA-Z0-9\s\,\\\/\.\'\"\)\(\[\]\{\}\-\&amp;]*$</constant-value>
    </constant>
    <!-- regular expression for organization names fields which allows number as starting characters-->
    <constant>
      <constant-name>checkOrgNameNumber</constant-name>
      <constant-value>^[a-zA-Z0-9]{1}[a-zA-Z0-9\s\,\\\/\.\'\"\)\(\[\]\{\}\-\&amp;]*$</constant-value>
    </constant>
    <!-- regular expression for person names fields --> 
    <constant>
      <constant-name>checkPersonName</constant-name>
      <constant-value>^[a-zA-Z]{1}[a-zA-Z0-9\s\.\']*$</constant-value>
    </constant> 
    <!-- regular expression for person names fields --> 
    <constant>
      <constant-name>checkwbPersonName</constant-name>
      <constant-value>^(([a-zA-Z0-9\s\,\\\/\.\'\)\(\[\]\{\}\?\-\@\#\!\$\^\*\%\&amp;]+[\w\s\,\\\/\.\'\)\(\[\]\{\}\?\-\@\#\!\$\^\*\%\&amp;]*)?)$|-</constant-value>
      
    </constant>   

    <!--regular expression for alpha numeric fields -->
    <constant>
      <constant-name>decimalValue</constant-name>
      <constant-value>^([0])*\d{0,8}(\.\d{1,2})?$</constant-value>
    </constant>

    <!--regular expression for interest fields -->
    <constant>
      <constant-name>interestValue</constant-name>
      <constant-value>^([0])*\d{0,2}(\.\d{1,2})?$</constant-value>
    </constant>

    <!--regular expression for big decimal fields -->
    <constant>
      <constant-name>bigdecimalValue</constant-name>
      <constant-value>^(([0])*[1-9]{1}\d{0,9}(\.\d{1,2})?)$|^[0]*\.\[1-9]{1}\d?$|^\s*$</constant-value>
    </constant>
     <constant>
      <constant-name>bigdecimalValuewithzero</constant-name>
      <constant-value>^([0-9]{1}\d{0,9}(\.\d{1,2})?)$|^[0]*\.\[1-9]{1}\d?$|^\s*$</constant-value>
    </constant>
    <constant>
      <constant-name>bigdecimalwholeValue</constant-name>
      <constant-value>^(([0])*[1-9]{1}\d{0,9}(\.[0]{1,2})?)$|^\s*$</constant-value>
    </constant>
    <!-- regular expression for alphabets -->
    <constant>
      <constant-name>alphabetic</constant-name>
      <constant-value>^[a-zA-Z]*$</constant-value>
    </constant>

    <!--regular expression for  numeric fields -->
    <constant>
      <constant-name>numericValue</constant-name>
      <constant-value>^[0-9]*$</constant-value>
    </constant>

    <!-- regular expression for Date in DD/MM/YYYY -->
    <constant>
      <constant-name>checkDate</constant-name>
      <constant-value>^\d{2}\/\d{2}\/\d{4}$</constant-value>
    </constant>
    <!-- regular expression for Time in HH:MM -->
    <constant>
      <constant-name>checkTime</constant-name>
      <constant-value>^(([0-9]|[0][0-9]|[0-1][0-2])\:([0-9]|[0-5][0-9]))$</constant-value>
    </constant>
    <!-- regular expression for E-mail-->
    <constant>
      <constant-name>checkEmail</constant-name>
	  <constant-value>^(([a-zA-Z0-9]+[\w\s\.\,\?\-]*@\w+[\w\s\.\?,]*)?)$|-</constant-value>
      <!--<constant-value>^([a-zA-Z]+[\w]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-zA-Z]{2,6}(?:\.[a-zA-Z]{2})?)$</constant-value>-->
    </constant>
    <!-- regular expression for commission-->
    <constant>
      <constant-name>commission</constant-name>
      <constant-value>^\d{0,2}?(\.\d{0,2})?$</constant-value>
    </constant>
    <!-- regular expression for Simple Interest-->
    <constant>
      <constant-name>simpleInterest</constant-name>
      <constant-value>^([0])*\d?(\.\d{1,2})?$</constant-value>
    </constant>
    <!-- regular expression for Clause Number t-->
    <constant>
      <constant-name>decimalAlphaNumeric</constant-name>
      <constant-value>^[a-zA-Z0-9]{1}[a-zA-Z0-9\.]*$</constant-value>
    </constant>
  </global>

